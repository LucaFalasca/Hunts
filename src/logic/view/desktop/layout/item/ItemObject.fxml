<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="ancPane" prefHeight="86.0" prefWidth="390.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="lbObjDesc" layoutX="88.0" prefHeight="100.0" prefWidth="110.0" text="Label" AnchorPane.bottomAnchor="0.0" AnchorPane.topAnchor="0.0" />
      <Label fx:id="lbObjectName" layoutY="42.0" prefHeight="100.0" prefWidth="88.0" text="Label" AnchorPane.bottomAnchor="0.0" AnchorPane.topAnchor="0.0" />
      <ImageView fx:id="imgObject" fitHeight="100.0" fitWidth="100.0" layoutX="188.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="btnModify" layoutX="294.0" layoutY="38.0" mnemonicParsing="false" onAction="#handleModifyHunt">
         <graphic>
            <ImageView fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../../../resources/edit.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="btnDelete" layoutX="345.0" layoutY="38.0" mnemonicParsing="false" onAction="#handleDeleteHunt">
         <graphic>
            <ImageView fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../../../resources/delete.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</AnchorPane>
