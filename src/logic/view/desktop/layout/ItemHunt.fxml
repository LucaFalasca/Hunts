<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="ancHunt" maxHeight="-Infinity" maxWidth="-Infinity" minWidth="150.0" prefHeight="38.0" prefWidth="435.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="lbHuntName" layoutX="14.0" layoutY="7.0" text="Label">
         <font>
            <Font size="16.0" />
         </font></Label>
      <Button fx:id="btnModifyHunt" layoutX="297.0" layoutY="7.0" mnemonicParsing="false" onAction="#handleModifyHunt">
         <graphic>
            <ImageView fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../../resources/edit.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="bntStart" layoutX="218.0" layoutY="7.0" mnemonicParsing="false" onAction="#handleStartGame" text="Start" />
      <Button fx:id="btnDeleteHunt" layoutX="366.0" layoutY="7.0" mnemonicParsing="false" onAction="#handleDeleteHunt">
         <graphic>
            <ImageView fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../../resources/delete.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</AnchorPane>
